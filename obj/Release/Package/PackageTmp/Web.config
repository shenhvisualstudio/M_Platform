<configuration>
    <system.web>
      <webServices>
        <protocols>
          <add name="HttpSoap"/>
          <add name="HttpPost"/>
          <add name="HttpGet"/>
          <add name="Documentation"/>
        </protocols>
      </webServices>    
      <compilation targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>
  
  <appSettings>
    <add key="DynamicIntervalTime" value="120"/>
  </appSettings>
  
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="mobileSoap" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://168.100.1.73/mobile/service.asmx" binding="basicHttpBinding"
                bindingConfiguration="mobileSoap" contract="ServiceSendMessage.mobileSoap"
                name="mobileSoap" />
        </client>
    </system.serviceModel>
</configuration>
